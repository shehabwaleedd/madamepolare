{
  "format": "custom",
  "id": "settings",
  "label": "Settings",
  "repeatable": false,
  "status": true,
  "json": {
    "Main": {
      "site_title": {
        "type": "Text",
        "config": {
          "label": "Site Title",
          "placeholder": "Enter site title (e.g. Madamepolare)"
        }
      },
      "site_logo": {
        "type": "Image",
        "config": {
          "label": "Logo",
          "constraint": {
            "width": 150,
            "height": 150
          },
          "thumbnails": []
        }
      },
      "nav_items": {
        "type": "Group",
        "config": {
          "label": "Navigation Links",
          "fields": {
            "link_label": {
              "type": "Text",
              "config": {
                "label": "Link Label",
                "placeholder": "Enter link text"
              }
            },
            "link_url": {
              "type": "Link",
              "config": {
                "label": "Link",
                "placeholder": "Select a link",
                "select": null
              }
            }
          }
        }
      },
      "horizontalStats": {
        "type": "Group",
        "config": {
          "label": "Statistics Rows",
          "repeat": true,
          "fields": {
            "row_title": {
              "type": "Text",
              "config": {
                "label": "Statistic Title",
                "placeholder": "Of the startups"
              }
            },
            "stat_number": {
              "type": "Text",
              "config": {
                "label": "Statistic Value",
                "placeholder": "95"
              }
            },
            "sign": {
              "type": "Text",
              "config": {
                "label": "Value Suffix",
                "placeholder": "%"
              }
            }
          }
        }
      },
      "marqueetext": {
        "type": "Text",
        "config": {
          "label": "MarqueeText",
          "placeholder": "Please Enter Marquee Text ( Must ) e.g (Welcome to our website)"
        }
      },
      "centeredtitle": {
        "type": "Text",
        "config": {
          "label": "centeredTitle",
          "placeholder": "Please Enter Centered Title ( Must ) e.g. ( Community Agency )"
        }
      },
      "double_button_title": {
        "type": "Text",
        "config": {
          "label": "Double Button Title",
          "placeholder": "Please Enter Button Title ( Must ) e.g. ( Let's Talk )"
        }
      }
    }
  }
}
